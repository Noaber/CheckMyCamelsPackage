{
   "name": "stijlgenoten/check-my-camels",
    "description": "Case sensitive classname checker for Laravel apps",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilfred Lepelaar",
            "email": "wilfredl@stijlgenoten.nl",
            "homepage": "https://stijlgenoten.nl"
        }
    ],
    "homepage": "https://github.com/Stijlgenoten/CheckMyCamelsPackage",
    "keywords": ["Laravel", "Classname", "CamelCase", "Tool", "Check"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Stijlgenoten\\CheckMyCamels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stijlgenoten\\CheckMyCamels\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stijlgenoten\\CheckMyCamels\\CheckMyCamelsServiceProvider"
            ],
            "aliases": {
                "CheckMyCamels": "Stijlgenoten\\CheckMyCamels\\Facades\\CheckMyCamels"
            }
        }
    }
}
